---
date: Last Modified
---

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ title }}</title>
  <link rel="icon" href="/favicon.svg" type="image/svg+xml">
  <link rel="stylesheet" href="{{ '/css/style.css' | url }}" />
  <link href="themes/prism-ghcolors.css" rel="stylesheet" />
</head>
<body>
  <header>
    {% include "nav.njk" %}
    {% if not (page.url == '/') %}
      <time datetime="{{ page.date.toISOString() }}">
        Updated {{ page.date.toLocaleString('en-US', {'dateStyle': 'medium'}) }}
      </time>
    {% endif %}

    <h1>{{ title | mdInline | safe }}</h1>

    {% if not raw_md %}
      {% if eleventyNavigation and eleventyNavigation.parent and not manual_toc %}
        <details class="toc-details">
          <summary class="toc-summary">Table of Contents</summary>
          {{ content | toc | safe }}
        </details>
      {% endif %}
    {% endif %}
  </header>


  <main>
    {{ content | typogr | safe }}
  </main>

  <footer>
    {% include "footer.njk" %}
  </footer>
</body>
</html>
